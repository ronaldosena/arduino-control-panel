  "license": "MIT",
  "devDependencies": {
    "electron": "^5.0.6",
    "electron-rebuild": "^1.8.5",
    "nodemon": "^1.19.1"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.9.0",
    "electron-window-state": "^5.0.3",
    "glob": "^7.1.4",
    "node-gyp": "^5.0.3",
    "serialport": "^7.1.5"
  }
